                if (passedID.indexOf('lot') == -1) {
                    //It's a Building
                    window.buildingMarkers[passedID].setMap(window.map);
                    openBuildingInfo(buildings[passedID]);
                } else {
                    //It's a Parking Lot
                    id = passedID.replace('lot', '');
                    window.parkingMarkers[id].setMap(window.map);
                    openParkingInfo(parkingLots[id]);

                    //Register keyhandler
                    $(".directionsInput").keyup(function (e) {
