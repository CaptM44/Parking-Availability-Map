        function showCategory(id, filter) {
            if (typeof filter == 'undefined') {
                filter = true;
            }
            closeInfoDiv();
            closeDirections();

            //Hide everything first
            $.each(window.buildingMarkers, function (i, marker) {
                marker.setMap(null);
            });

            $.each(window.parkingMarkers, function (i, marker) {
                marker.setMap(null);
            });

            //Wipe out the searchList so it's ready to populate
            window.searchList = [];

            posArr = [];
            //id 7 is parking lots, -1 means all buildings, anything else select by id and use the buildingCategories object
            if (id == 7) {
                $('#locationButton').html('Parking&nbsp<span class="caret"></span> ');
                $.each(window.parkingMarkers, function (i, marker) {
                    marker.setMap(window.map);
                    window.searchList.push(window.parkingLots[i]);
                    posArr.push({ lat: parkingLots[i].lat, lng: parkingLots[i].lng });
                });
            } else if (id == -1) {
                $('#locationButton').html('Locations&nbsp<span class="caret"></span>');
                $.each(window.buildingMarkers, function (i, marker) {
                    marker.setMap(window.map);
                    window.searchList.push(window.buildings[i]);
                    posArr.push({ lat: buildings[i].lat, lng: buildings[i].lng });
                });
                $.each(window.parkingMarkers, function (i, marker) {
                    marker.setMap(window.map);
                    window.searchList.push(window.parkingLots[i]);
                    posArr.push({ lat: parkingLots[i].lat, lng: parkingLots[i].lng });
                });
                $.each(window.departments, function (i, dept) {
                    window.searchList.push(dept);
                });
            } else {
                if (id !== 0) {
                    $('#locationButton').html(categories[id].name + '&nbsp<span class="caret"></span>');
                } else {
                    $('#locationButton').html('Locations&nbsp<span class="caret"></span>');
                }
                $.each(window.buildingCategories, function (i, building) {
                    //There may be some buildingCategories associated with inactive building.  Hence the undefined check
                    if (building.category_id == id && typeof window.buildingMarkers[building.building_id] !== 'undefined') {
                        window.buildingMarkers[building.building_id].setMap(window.map);
                        window.searchList.push(window.buildings[building.building_id]);
                        posArr.push({ lat: buildings[building.building_id].lat, lng: buildings[building.building_id].lng });
                    }
                });
            }

            latTot = 0;
            latCount = 0;
            lngTot = 0;
            lngCount = 0;
            minLat = 41.076;
            minLng = -81.512;
            maxLat = 41.076;
            maxLng = -81.512;

            $.each(posArr, function (i, arr) {
                latTot += Number(arr.lat);
                lngTot += Number(arr.lng);

                lngCount++;
                latCount++;

                if (minLat > arr.lat) {
                    minLat = arr.lat;
                }
                if (minLng > arr.lng) {
                    minLng = arr.lng;
                }
                if (maxLat < arr.lat) {
                    maxLat = arr.lat;
                }
                if (maxLng < arr.lng) {
                    maxLng = arr.lng;
                }
            });

            latLng = new google.maps.LatLng(latTot / latCount, lngTot / lngCount);
            window.map.panTo(latLng);

            // sw = new google.maps.LatLng(minLat,maxLng);
            // ne = new google.maps.LatLng(maxLat,minLng);
            // bounds = new google.maps.LatLngBounds(sw,ne);
            // window.map.fitBounds(bounds);
            if (filter) {
                filterSearch('');
            }
        }


        function openMarker(elem) {
            //It's a building
            if ($(elem).attr('id').indexOf('building') !== -1) {
                id = $(elem).attr('id').replace('building', '');
                window.buildingMarkers[id].setMap(window.map);
                openBuildingInfo(buildings[id]);

            } else {//It's a parking Lot
                id = $(elem).attr('id').replace('lot', '');
                window.parkingMarkers[id].setMap(window.map);
                openParkingInfo(parkingLots[id]);

                //Register keyhandler
                $(".directionsInput").keyup(function (e) {
                    if (e.keyCode == 13) {
                        getDirections($(".directionsInput").attr('id').replace('dirAddressLot', ''));
                    }
                });

            }
        }

        function resetMarkers() {
            //UnHighlight all other markers
            $.each(window.buildingMarkers, function (i, marker) {
                marker.setIcon();
                marker.setAnimation(null);
            });
            $.each(window.parkingMarkers, function (i, marker) {
                if (i == 25) {
                    marker.setIcon(icons.visitorParking);
                } else {
                    marker.setIcon(icons.parking);
                }
                marker.setAnimation(null);
            });
        }


        //Directions Service
        function getDirections(lot_number, buildingId) {
            if (typeof buildingId == 'undefined') {
                //end = parkingLots[lot_number].address+lot' akron ohio';
                end = new google.maps.LatLng(parkingLots[lot_number].lat, parkingLots[lot_number].lng);
                start = $('#dirAddressLot' + lot_number).val();
                dirType = "DRIVING";
            } else {
                end = new google.maps.LatLng(buildings[buildingId].lat, buildings[buildingId].lng);
                start = new google.maps.LatLng(parkingLots[lot_number].lat, parkingLots[lot_number].lng);
                dirType = "WALKING";
            }

            window.directionsRequest = {
                origin: start,
                destination: end,
                travelMode: google.maps.TravelMode[dirType]
            };

            window.directionsService.route(window.directionsRequest, function (response, status) {
                if (status == google.maps.DirectionsStatus.OK) {
                    directionsDisplay.setDirections(response);
                    directionsDisplay.setMap(map);
                    directionsDisplay.setPanel(document.getElementById('directionsPanelBody'));
                    $('#directionsPanel').addClass('col-sm-4');
                    $('#directionsPanel').show();
                    $('#infoPanel').hide();
                    $('#map-canvas').removeClass('col-sm-12');
                    $('#map-canvas').addClass('col-sm-8');
                    directionType(dirType);
                    resize();
                }
            });

        }

        //Directions Service
        function geoDirections(marker, id) {
            window.userHereBox.close();
            if (id.indexOf('lot') == -1) {
                //It's a building
                end = new google.maps.LatLng(buildings[id].lat, buildings[id].lng);
                start = marker.position;
                dirType = "WALKING";
            } else {
                //It's a parking lot
                end = new google.maps.LatLng(parkingLots[id.replace('lot', '')].lat, parkingLots[id.replace('lot', '')].lng);
                start = marker.position;
                dirType = "WALKING";
            }

            window.directionsRequest = {
                origin: start,
                destination: end,
                travelMode: google.maps.TravelMode[dirType]
            };

            window.directionsService.route(window.directionsRequest, function (response, status) {
                if (status == google.maps.DirectionsStatus.OK) {
                    directionsDisplay.setDirections(response);
                    directionsDisplay.setMap(map);
                    directionsDisplay.setPanel(document.getElementById('directionsPanelBody'));
                    $('#directionsPanel').addClass('col-sm-4');
                    $('#directionsPanel').show();
                    $('#infoPanel').hide();
                    $('#map-canvas').removeClass('col-sm-12');
                    $('#map-canvas').addClass('col-sm-8');
                    directionType(dirType);
                    resize();
                }
            });

        }

        function directionType(type) {
            window.directionsRequest.travelMode = google.maps.TravelMode[type];
            window.directionsService.route(window.directionsRequest, function (response, status) {
                if (status == google.maps.DirectionsStatus.OK) {
                    directionsDisplay.setDirections(response);
                    $('.directionButtons').each(function () {
                        $(this).removeClass('disabled');
                    });
                    $('#directions' + type).addClass('disabled');
                }
            });
        }

        function closeDirections() {
            $('#directionsPanel').hide();
            $('#directionsPanel').removeClass('col-sm-4');
            $('#directionsPanelBody').html('');
            $('#map-canvas').removeClass('col-sm-8');
            $('#map-canvas').addClass('col-sm-12');
            directionsDisplay.setMap(null);
            resetMarkers();
            resize();
        }

        //Search Filtering Business

        var selection = -1;
        var prev = -1;

        $('#dropDownSearch').keyup(function (e) {
            if (e.keyCode !== 38 && e.keyCode !== 40) {
                filterSearch($(this).val());
                selection = -1;
            }
        });

        $('#dropDownSearch').keyup(function (e) {
            var dropDownOptionsArr = [];
            $('.dropDownOptions').each(function (i, thisOption) {
                dropDownOptionsArr[i] = $(this).attr('id');
            });

            if (e.keyCode == 38 || e.keyCode == 40) {

                if (e.keyCode == 38) {
                    if (selection == -1) {
                        $('#' + dropDownOptionsArr[0]).css('background-color', '#428bca');
                        $('#' + dropDownOptionsArr[0]).css('color', '#fff');
                        selection = 0;
                    } else if (selection == 0) {
                        $('#' + dropDownOptionsArr[0]).css('background-color', '#fff');
                        $('#' + dropDownOptionsArr[0]).css('color', '#333');
                        $('#' + dropDownOptionsArr[dropDownOptionsArr.length - 1]).css('background-color', '#428bca');
                        $('#' + dropDownOptionsArr[dropDownOptionsArr.length - 1]).css('color', '#fff');
                        selection = dropDownOptionsArr.length - 1;
                    } else {
                        $('#' + dropDownOptionsArr[selection]).css('background-color', '#fff');
                        $('#' + dropDownOptionsArr[selection]).css('color', '#333');
                        $('#' + dropDownOptionsArr[(selection - 1) % dropDownOptionsArr.length]).css('background-color', '#428bca');
                        $('#' + dropDownOptionsArr[(selection - 1) % dropDownOptionsArr.length]).css('color', '#fff');
                        selection = (selection - 1) % dropDownOptionsArr.length;
                    }
                } else if (e.keyCode == 40) {
                    if (selection == -1) {
                        $('#' + dropDownOptionsArr[0]).css('background-color', '#428bca');
                        $('#' + dropDownOptionsArr[0]).css('color', '#fff');
                        selection = 0;
                    } else {
                        $('#' + dropDownOptionsArr[selection]).css('background-color', '#fff');
                        $('#' + dropDownOptionsArr[selection]).css('color', '#333');
                        $('#' + dropDownOptionsArr[(selection + 1) % dropDownOptionsArr.length]).css('background-color', '#428bca');
                        $('#' + dropDownOptionsArr[(selection + 1) % dropDownOptionsArr.length]).css('color', '#fff');
                        selection = (selection + 1) % dropDownOptionsArr.length;
                    }
                }
            } else if (e.keyCode == 13) {
                if (selection >= 0) {
                    $('#dropDownList').parent().removeClass('open');
                    $('#dropDownSearch').val($('#' + dropDownOptionsArr[selection]).text());
                    openMarker($('#' + dropDownOptionsArr[selection]));
                } else {
                    $('#dropDownList').parent().removeClass('open');
                    $('#dropDownSearch').val($('#' + dropDownOptionsArr[0]).text());
                    openMarker($('#' + dropDownOptionsArr[0]));
                }
            }
        });

        //When passed input, narrow the dropdown search list based on input
        //Resize list and page before exit
        function filterSearch(input) {

            $('#dropDownList').css('height', 'auto');
            $('#dropDownList').css('overflow', 'scroll');

            var matches = [];
            window.searchList.sort(alphaComp);
            $.each(window.searchList, function (i, item) {
                var reg = new RegExp(input.split('').join('\\w*').replace(/\W/, ""), 'i');
                //Match Names
                if (item.name.match(reg)) {
                    matches[i] = item.name;
                }
                //Match Abbreviations
                if (typeof item.abbreviation !== 'undefined' && item.abbreviation !== null) {
                    if (item.abbreviation.match(reg)) {
                        matches[i] = item.name;
                    }
                }
                //Match Lot Numbers
                if (typeof item.lot_number !== 'undefined') {
                    if (item.lot_number.match(reg)) {
                        matches[i] = item.name;
                    }
                }

            });

            $('#dropDownList').html('');
            $.each(matches, function (id, text) {
                if (typeof text !== 'undefined')
                    if (typeof window.searchList[id].lights == 'undefined') {
                        //It's either a building or a department
                        if (typeof window.searchList[id].order == 'undefined') {
                            //It's a Building!
                            if (window.searchList[id].abbreviation !== null) {
                                abbrev = ' (' + window.searchList[id].abbreviation + ') ';
                            } else {
                                abbrev = '';
                            }

                            $('#dropDownList').append('<li><a id="building' + window.searchList[id].id + '" class="dropDownOptions" role="menuitem" href="#" onclick="openMarker(this)">' + window.searchList[id].name.substring(0, 39) + abbrev + '</a></li>');
                        } else {
                            //It's a department!
                            $('#dropDownList').append('<li><a id="building' + window.searchList[id].building_id + '" class="dropDownOptions" role="menuitem" href="#" onclick="openMarker(this)">' + window.searchList[id].name.substring(0, 39) + '</a></li>');
                        }
                    } else {
                        //It's a parking lot!
                        $('#dropDownList').append('<li><a id="lot' + window.searchList[id].id + '" class="dropDownOptions" role="menuitem" href="#" onclick="openMarker(this)">' + window.searchList[id].name.substring(0, 39) + ' (Lot ' + window.searchList[id].lot_number + ')</a></li>');
                    }
            });

            setTimeout(function () { resize() }, 100);
            setTimeout(function () { resizeList(input) }, 100);
        }

        function resizeList(input) {
            if (input.length > 0) {
                $('#dropDownList').parent().addClass('open');
                $('#dropDownList').removeClass('display');
            }

            if ($('#dropDownList').height() >= .8 * screen.height) {
                $('#dropDownList').css('height', '' + (screen.height * .8) + 'px');
                $('#dropDownList').css('overflow', 'scroll');
            } else {
                $('#dropDownList').css('overflow', 'auto');
                $('#dropDownList').css('height', '');
            }
        }

        function alphaComp(a, b) {
            if (a.name < b.name)
                return -1;
            if (a.name > b.name)
                return 1;
            return 0;
        }



    </script>

