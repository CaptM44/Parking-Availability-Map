                    content: content
                });


                options = {
                    enableHighAccuracy: true,
                    timeout: 5000,
                    maximumAge: 0
                };

                window.isFirstGeo = true;

                function updatePos(pos) {
                    window.loc = new google.maps.LatLng(pos.coords.latitude, pos.coords.longitude);
                    if (window.isFirstGeo && pos.coords.accuracy < 150) {
                        window.userMarker.setPosition(window.loc);
                        window.userMarker.setMap(window.map);
                        window.isFirstGeo = false;
                        window.userMarker.setAnimation(google.maps.Animation.BOUNCE);
                        setTimeout(function () { window.userMarker.setAnimation(null); }, 2200);
                        window.lastPos = window.loc;
                        return;
                    }

                    if (pos.coords.accuracy < 150 && typeof window.lastPos !== 'undefined' && (Math.abs((Number(window.lastPos.lat()) - Number(window.loc.lat()))) > .0005 || Math.abs((Number(window.lastPos.lng()) - Number(window.loc.lng()))) > .0005)) {
                        window.userMarker.setPosition(window.loc);
                        window.userMarker.setMap(window.map);
                        window.userMarker.setAnimation(google.maps.Animation.BOUNCE);
                        setTimeout(function () { window.userMarker.setAnimation(null); }, 2200);
                        window.lastPos = window.loc;
                        return;
                    } else {
                        window.lastPos = window.loc;
                        return;
                    }
                }

                function failPos() {
                    return;
                }

                window.geoLocation = navigator.geolocation.watchPosition(updatePos, failPos, options);
            }
        }

