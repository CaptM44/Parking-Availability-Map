            initInfoBoxes();

            initGeolocation();

            showCategory(0, false);
            //Despite only showing the default assortment of markers, populate the search list with a full list of locations
            window.searchList = [];
            $.each(buildings, function (i, building) {
                window.searchList.push(building);
            });
            $.each(parkingLots, function (i, lot) {
